[application]

config/name="Turismo X-Treme"
run/main_scene="res://lobby/lobby.tscn"
boot_splash/image="res://assets/Icon.png"
boot_splash/bg_color=Color( 0.192157, 0.192157, 0.192157, 1 )
config/icon="res://icon.png"

[autoload]

Config="*res://server/config.gd"
History="*res://server/history.gd"
Sync="*res://server/sync.gd"
Players="*res://server/players.gd"
Server="*res://server/server.gd"

[debug]

multirun/add_custom_args=true
multirun/first_window_args=""
multirun/other_window_args=""
multirun/number_of_windows=2
multirun/window_distance=10

[display]

window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg", "res://addons/multirun/plugin.cfg", "res://addons/scatter/plugin.cfg", "res://addons/smoothing/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg" )

